{
    "name": "efabrica/neoforms",
    "description": "Better way to render forms",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "nette/forms": "^3.1",
        "latte/latte": "^2.11 || ^3.0"
    },
    "config": {
        "platform": {
            "php": "8.3.9"
        }
    },
    "autoload": {
        "psr-4": {
            "Efabrica\\NeoForms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Efabrica\\NeoForms\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Richard Popelis",
            "email": "richard@popelis.sk"
        }
    ],
    "require-dev": {
        "nette/application": "^3.2",
        "radekdostal/nette-datetimepicker": "^3.1",
        "tracy/tracy": "^2.9",
        "nette/database": "^3.1",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpunit/phpunit": "^10.3"
    }
}
